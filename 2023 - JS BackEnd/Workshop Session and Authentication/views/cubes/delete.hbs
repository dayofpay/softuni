
        <main>
            {{cubeId}}

            <h1>Delete Cube</h1>
            <div class="form">
                <form action="/cubes/delete" method="POST">
                    <label for="name">Name</label>
                    <input type="text" id="name" name="name" value="{{cube.name}}" disabled="true" />
                    <label for="description">Description</label>
                    <textarea id="description" name="description" disabled="true" />{{cube.description}}</textarea>
                    <label for="image">ImageUrl</label>
                    <input type="text" id="image" name="imageUrl" value="{{cube.image_Url}}" disabled="true" />
                    <label for="difficulty">Difficulty</label>
                    <select id="difficulty" name="difficultyLevel" value="{{cube.diff_level}}" disabled="true" />
                {{#each options}}
                    <option value="{{value}}" {{#if selected}}selected{{/if}}>{{title}}</option>
                {{/each}}
                    </select>
                    <input type="submit" value="Delete">
                </form>
            </div>
        </main>